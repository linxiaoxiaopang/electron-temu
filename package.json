{
  "name": "electron-temu",
  "version": "0.1.0",
  "private": true,
  "description": "正丁",
  "authors": "正丁",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "inspect": "cross-env NODE_ENV=development electron-forge start -- --inspect=9229",
    "inspect-brk": "cross-env NODE_ENV=development electron-forge start -- --inspect-brk=9229",
    "package": "cross-env NODE_ENV=production electron-forge package --platform=win32",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "make": "electron-forge make --platform=win32"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "body-parser": "^2.2.0",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dayjs-plugin-utc": "^0.1.2",
    "dayjs-timezone-iana-plugin": "^0.2.0",
    "electron-squirrel-startup": "^1.0.1",
    "eventemitter3": "^5.0.1",
    "express": "^5.1.0",
    "get-port": "^7.1.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "vue": "^2.6.14",
    "vue-router": "^3.0.2",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-wix": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
    "@electron-forge/plugin-fuses": "^7.8.3",
    "@electron/fuses": "^1.8.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "chokidar": "^4.0.3",
    "cross-env": "^10.0.0",
    "electron": "^37.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": "off",
      "no-unused-vars": "off",
      "no-unreachable": "off"
    }
  },
  "_moduleAliases": {
    "~": "./background",
    "~express": "./background/express",
    "~model": "./background/model",
    "~store": "./background/store",
    "~utils": "./background/utils"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
